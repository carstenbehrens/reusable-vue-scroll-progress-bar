{
  "name": "reusable-vue-scroll-progress-bar",
  "description": "A simple reusable Vue 3 scroll progress component",
  "version": "1.0.0",
  "private": true,
  "author": "Carsten Behrens <carsten@carstenbehrens.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "cypress run-ct --quiet --reporter spec",
    "test:dev": "cypress open-ct",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "main": "./dist/reusable-vue-scroll-progress-bar-umd.js",
  "module": "./dist/reusable-vue-scroll-progress-bar-es.js",
  "exports": {
    ".": {
      "import": "./dist/reusable-vue-scroll-progress-bar-es.js",
      "require": "./dist/reusable-vue-scroll-progress-bar-umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "vue 3",
    "scroll",
    "progress",
    "bar"
  ],
  "dependencies": {
    "lodash.throttle": "^4.1.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "cypress": "^9.5.4",
    "husky": "^7.0.4",
    "vite": "^2.9.5",
    "vitepress": "^0.22.3"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carstenbehrens/reusable-vue-scroll-progress-bar.git"
  },
  "bugs": {
    "url": "https://github.com/carstenbehrens/reusable-vue-scroll-progress-bar/issues"
  },
  "homepage": "https://github.com/carstenbehrens/reusable-vue-scroll-progress-bar#readme"
}
